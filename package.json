{
  "name": "dockerium",
  "version": "0.0.1",
  "repository": "git@github.com:Rechdan/Dockerium.git",
  "author": "Nelson Fai√ßal Rechdan <rechdanfr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "upgrade": "ncu --reject typescript -u && yarn --check-files",
    "dev": "nodemon -w \"next.config.js\" -e \"*\" -x \"yarn run dev:run\"",
    "dev:run": "next dev",
    "dev:local:docker": "docker build -t dockerium:dev . && docker run -v /var/run/docker.sock:/var/run/docker.sock -v /usr/bin/docker:/usr/bin/docker -p 3000:3000 dockerium:dev",
    "dev:prisma": "nodemon -w \"./prisma/schema.prisma\" -e \"*\" -x \"clear && yarn prisma db push --accept-data-loss\"",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@prisma/client": "4.8.0",
    "argon2": "0.30.2",
    "axios": "1.2.2",
    "cookie": "0.5.0",
    "dockerode": "3.3.4",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "9.1.1",
    "styled-components": "5.3.6",
    "swr": "2.0.0",
    "yup": "0.32.11",
    "zustand": "4.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.1.1",
    "@types/cookie": "0.5.1",
    "@types/dockerode": "3.3.14",
    "@types/jsonwebtoken": "9.0.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "eslint": "8.31.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-perf": "3.3.1",
    "nodemon": "2.0.20",
    "prisma": "4.8.0",
    "ts-toolbelt": "9.6.0",
    "typescript": "4.8.4"
  }
}
